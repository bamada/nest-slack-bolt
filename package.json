{
  "name": "nestjs-slack-bolt",
  "version": "1.1.0",
  "description": "A Nestjs module using Slack Bold SDK",
  "author": "bamada",
  "license": "MIT",
  "url": "https://github.com/bamada/nest-slack-bolt#readme",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-angular": "19.1.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "3.2.1",
    "@nestjs/core": "^10.1.3",
    "@slack/bolt": "3.17.1",
    "husky": "9.0.11",
    "reflect-metadata": "0.2.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.7",
    "@release-it/conventional-changelog": "^8.0.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.3",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "release-it": "^17.0.0",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.1.3",
    "@nestjs/core": "^9.0.0 || ^10.1.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.2.0"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bamada/nest-slack-bolt"
  }
}
